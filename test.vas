section .data
message: db "Hello world!\n"

section .text
return:
    mov rax, 60
    imul rdi, rax, 2
    syscall

true:
    call return

false:
    mov rax, 0
    xchg rdi, rax
    lea rbx, [rel return]
    call rbx

_start:
    mov rax, 1
    mov dil, al
    lea rsi, [rel message]
    mov rdx, 13
    syscall
    
    mov r11, 2
    test r11, 6
    je true
    jmp false